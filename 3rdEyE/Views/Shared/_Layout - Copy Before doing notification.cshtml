<script>
    $(document).ready(function () {
        $('#ALERT_SECTION').hide();
        $('#modal_message').hide();

        //#MANAGING ALERT MESSAGE:START
        @if (TempData["ALERT_MESSAGE_TYPE"] !=null){

            // get title and body from backend
            var ALERT_MESSAGE_TITLE = (string)TempData["ALERT_MESSAGE_TITLE"];
            var ALERT_MESSAGE_BODY = (string)TempData["ALERT_MESSAGE_BODY"];

            // set title, body in html
            <text>
            $('#ALERT_MESSAGE_TITLE').text("@ALERT_MESSAGE_TITLE");
            $('#ALERT_MESSAGE_BODY').text("@ALERT_MESSAGE_BODY");
            </text>

            var ALERT_MESSAGE_TYPE = (string)TempData["ALERT_MESSAGE_TYPE"];

            // set alert type wise class view and show
            if (ALERT_MESSAGE_TYPE == "INFORMATION")
            {
                <text>
            $('#ALERT_DIV').attr('class', "callout callout-info");
                $('#ALERT_SECTION').show();
                </text>
            }
            else if (ALERT_MESSAGE_TYPE == "DANGER")
            {
                <text>
                $('#ALERT_DIV').attr('class', "callout callout-danger");
                //$('#ALERT_SECTION').show(1000).delay(2000).hide(1000);
                $('#ALERT_SECTION').delay(500).show(500).delay(3000).slideUp(500);
                </text>
            }
            else if (ALERT_MESSAGE_TYPE == "WARNING")
            {
                <text>
                $('#ALERT_DIV').attr('class', "callout callout-warning");
                $('#ALERT_SECTION').delay(500).show(500).delay(3000).slideUp(500);
                </text>
            }
            else if (ALERT_MESSAGE_TYPE == "SUCCESS")
            {
                <text>
                $('#ALERT_DIV').attr('class', "callout callout-success");
                $('#ALERT_SECTION').delay(500).show(500).delay(3000).slideUp(500);
                </text>
            }
        }
        //#MANAGING ALERT MESSAGE:END

    });
</script>
