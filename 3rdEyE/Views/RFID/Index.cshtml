@model IEnumerable<_3rdEyE.Models.RFID_Log>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    
    <table class="table" style="">
        <tr>
            <th>
                #sl
            </th>
            <th>
                #time
            </th>
            <th>
                #res
            </th>
            <th>
                #found
            </th>
            <th>
                #s.stas
            </th>
            <th>
                #s.vehicle
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                   # @Html.DisplayFor(modelItem => item.PK_RFID_Log)
                </td>
                <td>
                   # @Html.DisplayFor(modelItem => item.CreatedAt)
                </td>
                <td>
                   # @Html.DisplayFor(modelItem => item.ReceivedValue)
                </td>
                <td>
                   # @Html.DisplayFor(modelItem => item.FoundCount)
                </td>
                <td>
                   # @Html.DisplayFor(modelItem => item.ReturnedStatus)
                </td>
                <td>
                   # @Html.DisplayFor(modelItem => item.ReturnedVehicle)
                </td>
            </tr>
        }

    </table>
</body>
</html>
